#include<stdio.h>
#include<conio.h>
int main()
{
	int num1,num2;
	
	printf("enter two number");
	scanf("%d%d",&num1,&num2);
	
	printf("addition two number", add(num1,num2));
	
	return (0);
}
int add(int num1,int num2);

if (!num1)
{
return num2;
}
else
{
	 return add((num1 & num2) << 1, num1 ^ num2);
}
